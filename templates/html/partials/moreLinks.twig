<nav class="dropdown">
    <button class="btn btn-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
        {{ _('more') }}
    </button>
    <ul class="dropdown-menu">
        {% for action, link in moreLinks %}
        <li><a href="{{ link.url }}" class="dropdown-item {{ action }}">{{ link.label }}</a></li>
        {% endfor %}
    </ul>
</nav>
