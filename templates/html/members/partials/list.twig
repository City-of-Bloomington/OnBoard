<table class="usa-table usa-table--borderless">
    <thead>
        <tr>
            <th>{{ _('name') }}</th>
            <th>{{ _('startDate') }}</th>
            <th>{{ _('endDate') }}</th>
        </tr>
    </thead>
    <tbody>
    {% for m in members %}
    <tr><td><a class="usa-link" href="{{ uri('people.view', {id: m.getPerson_id()}) }}">{{ m.getPerson().getFullname() }}</a></td>
        <td class="font-mono-3xs">{{ m.getStartDate|date('m/d/Y') }}</td>
        <td class="font-mono-3xs">{{ m.getEndDate|date('m/d/Y') }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
