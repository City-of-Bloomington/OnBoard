{% extends "html/layouts/default.twig" %}
{% block content %}
{{ include('html/committees/breadcrumbs.twig') }}
<section>
    <header><h1>{{ _('committees_past') }}</h1></header>
    <table class="table">
    {% for c in committees %}
    <tr><td><a href="{{ uri('committees.info') }}?committee_id={{ c.id }}">{{ c.name }}</a></td>
        <td>{{ c.endDate }}</td>
    </tr>
    {% endfor %}
    </table>
    <a class="btn btn-primary" href="{{ BASE_URI }}">{{ _('committees_current') }}</a>
</section>

{% endblock %}
