{% extends "html/layouts/default.twig" %}
{% block content %}
<section class="grid-container">
    <header>
        <h1>{{ _('committees_past') }}</h1>
    </header>
    <table class="usa-table">
        <thead>
            <tr><th>Name</th>
                <th>{{ _('endDate') }}</th>
            </tr>
        </thead>
    {% for c in committees %}
    <tr><td><a class="usa-link" href="{{ uri('committees.info', { committee_id: c.id }) }}">{{ c.name }}</a></td>
        <td>{{ c.endDate }}</td>
    </tr>
    {% endfor %}
    </table>
    <a class="usa-link" href="{{ BASE_URI }}">{{ _('committees_current') }}</a>
</section>
{% endblock %}
