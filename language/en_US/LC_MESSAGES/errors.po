msgid "invalidDate"
msgstr "You provided an invalid date."

msgid "invalidEndDateBeforeStart"
msgstr "The end date must come after the start date"

msgid "invalidEndDateAfterTerm"
msgstr "The end date must come before the end of the term"

msgid "invalidLogin"
msgstr "The username and password combination you entered is not correct."

msgid "missingRequiredFields"
msgstr "You did not include all the required fields."

msgid "noAccessAllowed"
msgstr "You are not allowed to go here."

msgid "notLoggedIn"
msgstr "You must be logged in to view this content."

msgid "passwordCorrupted"
msgstr "Your password is corrupted.  Please contact a system administrator for help."

msgid "ldap/unknownUser"
msgstr "That username is not in LDAP.  You cannot set LDAP authentication if the user is not in LDAP."

msgid "files/unknownFileType"
msgstr "The file you uploaded is not a valid filetype.  We only accept %s"

msgid "files/missingExtension"
msgstr "The file you uploaded is not a known filetype. Filenames must include an extension."

msgid "terms/overlappingTerms"
msgstr "You have entered an overlapping term.  A person cannot serve two terms, for the same committee, at the same time."

msgid "votes/notEnabled"
msgstr "Vote tracking is not enabled"
